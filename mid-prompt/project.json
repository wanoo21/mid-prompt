{
  "name": "mid-prompt",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "./mid-prompt/src",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": [
        "{options.outputPath}",
        "dist/./mid-prompt/.nitro",
        "dist/./mid-prompt/ssr",
        "dist/./mid-prompt/analog"
      ],
      "options": {
        "main": "./mid-prompt/src/main.ts",
        "configFile": "./mid-prompt/vite.config.ts",
        "outputPath": "dist/./mid-prompt/client"
      },
      "defaultConfiguration": "production",
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "sourcemap": false,
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "mid-prompt:build",
        "port": 4200
      },
      "configurations": {
        "development": {
          "buildTarget": "mid-prompt:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "mid-prompt:build:production"
        }
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "mid-prompt:build"
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["./mid-prompt/coverage"]
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["./mid-prompt/**/*.{ts,html}"]
      }
    }
  },
  "tags": []
}
